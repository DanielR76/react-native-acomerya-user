{
  "expo": {
    "name": "AComerYa",
    "slug": "acomerya",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#DADADA"
    },
    "updates": {
